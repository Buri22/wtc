{
    "name": "work_time_counter",
    "version": "0.2.0",
    "dependencies": {
        "axios": "^0.18.0",
        "bootstrap": "3.3.7",
        "node-less-chokidar": "^0.1.2",
        "npm-run-all": "^4.1.3",
        "react": "16.4.1",
        "react-bootstrap": "0.32.1",
        "react-dom": "16.4.1",
        "react-scripts": "1.1.4"
    },
    "scripts": {
        "build-css": "node-less-chokidar src/css/ -o src/css/",
        "watch-css": "npm run build-css && node-less-chokidar src/css/ -o src/css/ --watch --recursive",
        "start-js": "react-scripts start",
        "build-js": "react-scripts build",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm-run-all -p build-css build-js",
        "test": "react-scripts test --env=jsdom"
    }
}
