{
    "name": "work_time_counter",
    "version": "0.2.0",
    "devDependencies": {
        "babel-core": "^6.2.1",
        "babel-loader": "^6.2.0",
        "babel-preset-es2015": "^6.1.18",
        "babel-preset-react": "^6.1.18",
        "webpack": "^1.12.9",
        "webpack-dev-server": "^1.14.0"
    },
    "dependencies": {
        "babel-preset-stage-1": "^6.1.18",
        "axios": "^0.18.0",
        "bootstrap": "3.3.7",
        "node-less-chokidar": "^0.1.2",
        "npm-run-all": "^4.1.3",
        "react": "16.4.1",
        "react-bootstrap": "0.32.1",
        "react-dom": "16.4.1"
    },
    "scripts": {
        "build-css": "node-less-chokidar src/css/ -o src/css/",
        "watch-css": "npm run build-css && node-less-chokidar src/css/ -o src/css/ --watch --recursive",
        "start": "npm-run-all -p watch-css node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
        "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
        "build": "npm-run-all -p build-css build-js"
    }
}
